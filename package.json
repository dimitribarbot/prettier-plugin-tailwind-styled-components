{
  "name": "prettier-plugin-tailwind-styled-components",
  "version": "0.0.3",
  "description": "A Prettier plugin for sorting and formatting Tailwind CSS classes when using Tailwind Styled-Components.",
  "license": "MIT",
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/dimitribarbot/prettier-plugin-tailwind-styled-components"
  },
  "bugs": {
    "url": "https://github.com/dimitribarbot/prettier-plugin-tailwind-styled-components/issues"
  },
  "keywords": [
    "prettier",
    "tailwind-styled-components"
  ],
  "scripts": {
    "_pre": "rimraf dist",
    "_esbuild": "node build.mjs",
    "prebuild": "yarn _pre",
    "build": "yarn _esbuild --minify",
    "predev": "yarn _pre",
    "dev": "yarn _esbuild --watch",
    "test": "jest",
    "prepublishOnly": "yarn build && npm test",
    "format": "prettier \"src/**/*.js\" \"tests/test.js\" --write --print-width 100 --single-quote --no-semi --plugin-search-dir ./tests"
  },
  "devDependencies": {
    "@tailwindcss/line-clamp": "^0.4.2",
    "@trivago/prettier-plugin-sort-imports": "^3.4.0",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.10",
    "@types/styled-components": "^5.1.26",
    "esbuild": "^0.18.0",
    "escalade": "^3.1.1",
    "import-fresh": "^3.3.0",
    "import-from": "^4.0.0",
    "import-sort-style-module": "^6.0.0",
    "jest": "^29.3.1",
    "object-hash": "^3.0.0",
    "prettier": "^2.8.8",
    "prettier-plugin-css-order": "^1.3.0",
    "prettier-plugin-import-sort": "^0.0.7",
    "prettier-plugin-jsdoc": "^0.4.2",
    "prettier-plugin-organize-attributes": "^0.0.5",
    "prettier-plugin-organize-imports": "^3.2.1",
    "prettier-plugin-style-order": "^0.2.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rimraf": "^4.0.7",
    "styled-components": "^5.3.6",
    "tailwind-styled-components": "^2.2.0",
    "tailwindcss": "^3.2.4",
    "typescript": "^4.9.4"
  },
  "peerDependencies": {
    "@trivago/prettier-plugin-sort-imports": "*",
    "prettier": "^2.2 || ^3.0",
    "prettier-plugin-css-order": "*",
    "prettier-plugin-import-sort": "*",
    "prettier-plugin-jsdoc": "*",
    "prettier-plugin-organize-attributes": "*",
    "prettier-plugin-organize-imports": "*",
    "prettier-plugin-style-order": "*",
    "tailwind-styled-components": ">=2.2.0"
  },
  "peerDependenciesMeta": {
    "@trivago/prettier-plugin-sort-imports": {
      "optional": true
    },
    "prettier-plugin-css-order": {
      "optional": true
    },
    "prettier-plugin-import-sort": {
      "optional": true
    },
    "prettier-plugin-jsdoc": {
      "optional": true
    },
    "prettier-plugin-organize-attributes": {
      "optional": true
    },
    "prettier-plugin-organize-imports": {
      "optional": true
    },
    "prettier-plugin-style-order": {
      "optional": true
    }
  },
  "engines": {
    "node": ">=12.17.0"
  },
  "importSort": {
    ".js, .jsx, .ts, .tsx": {
      "style": "module"
    }
  },
  "jest": {
    "testTimeout": 15000
  }
}
